{
	"name": "kotisivu-block-theme",
	"version": "1.0.0",
	"description": "Lightweight block theme",
	"author": "Hekki Vihersalo",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build",
		"start": "wp-scripts start",
		"zip": "wp-scripts plugin-zip",
		"packages-update": "wp-scripts packages-update",
		"make-pot": "wp i18n make-pot . languages/kotisivu-block-theme.pot --slug=kotisivu-block-theme --domain=kotisivu-block-theme --exclude=src/blocks/static,src/blocks/dynamic/*/*.js,src/features,src/utils,src/hooks,src/__template-library__",
		"make-json": "wp i18n make-json languages/ --no-purge"
	},
	"devDependencies": {
		"@babel/core": "^7.23.0",
		"@wordpress/babel-preset-default": "^7.35.0",
		"@wordpress/block-editor": "^12.19.1",
		"@wordpress/blocks": "^12.28.1",
		"@wordpress/dependency-extraction-webpack-plugin": "^5.2.0",
		"@wordpress/i18n": "^4.51.0",
		"@wordpress/scripts": "^27.2.0",
		"babel": "^6.23.0",
		"babel-loader": "^9.1.3",
		"clean-webpack-plugin": "^4.0.0",
		"css-loader": "^6.8.1",
		"css-minimizer-webpack-plugin": "^5.0.1",
		"mini-css-extract-plugin": "^2.7.6",
		"style-loader": "^3.3.3",
		"terser-webpack-plugin": "^5.3.9",
		"webpack-bundle-analyzer": "^4.9.1",
		"webpack-livereload-plugin": "^3.0.2"
	},
	"files": [
		".vscode",
		"build",
		"inc",
		"languages",
		"parts",
		"public",
		"src",
		"templates",
		".babelrc",
		".editorconfig",
		".gitignore",
		"blocks.json",
		"config.json",
		"functions.php",
		"index.php",
		"jsconfig.json",
		"LICENSE",
		"package.json",
		"README.md",
		"style.css",
		"theme.json",
		"webpack.config.js",
		"yarn.lock"
	]
}
