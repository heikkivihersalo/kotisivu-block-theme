  "keywords": [
    "container, icon, list"
  ],
  "textdomain": "kotisivu-block-theme",
  "supports": {
    "className": false
  },
  "parent": [
    "ksd/contact-info-list"
  ],
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "content": {
      "type": "string",
      "default": ""
    },
    "type": {
      "type": "string",
      "default": "email"
    },
    "post": {
      "type": "object"
    },
    "linkUrl": {
      "type": "string",
      "default": ""
    },
    "linkTitle": {
      "type": "string",
      "default": ""
    },
    "linkTarget": {
      "type": "string"
    },
    "linkRel": {
      "type": "string"
