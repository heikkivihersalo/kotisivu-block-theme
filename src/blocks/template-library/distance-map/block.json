{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ksd/distance-map",
	"title": "Distance Map",
	"category": "blocks",
	"icon": "location-alt",
	"description": "Column layout of the map that shows distance to services",
	"keywords": ["map"],
	"textdomain": "kotisivu-block-theme",
	"supports": {
		"className": false
	},
	"editorScript": "file:../../../../build/distance-map.js",
	"editorStyle": "file:../../../../build/distance-map.css",
	"style": "file:../../../../build/style-distance-map.css",
	"attributes": {
		"template": {
			"type": "array",
			"default": []
		},
		"templateLock": {
			"type": ["string", "boolean"],
			"enum": ["all", "insert", false],
			"default": true
		},
		"align": {
			"type": "string",
			"default": "full"
		}
	}
}
