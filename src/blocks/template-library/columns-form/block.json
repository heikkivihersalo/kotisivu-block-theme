{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ksd/columns-form",
	"title": "Form | Image Left",
	"category": "blocks",
	"icon": "columns",
	"description": "Form section with image on left",
	"keywords": ["container, columns, column, 50-50, image left"],
	"textdomain": "kotisivu-block-theme",
	"supports": {
		"className": false
	},
	"editorScript": "file:../../../../build/columns-form.js",
	"editorStyle": "file:../../../../build/columns-form.css",
	"style": "file:../../../../build/style-columns-form.css",
	"attributes": {
		"template": {
			"type": "array",
			"default": []
		},
		"templateLock": {
			"type": ["string", "boolean"],
			"enum": ["all", "insert", false],
			"default": true
		},
		"align": {
			"type": "string",
			"default": "full"
		}
	}
}
