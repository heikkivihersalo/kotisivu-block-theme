{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ksd/contact-info-list-item",
	"title": "Contact Info List Item",
	"category": "widgets",
	"icon": "list-view",
	"description": "Icon list with insertable image",
	"keywords": ["container, icon, list"],
	"textdomain": "kotisivu-block-theme",
	"supports": {
		"className": false
	},
	"parent": ["ksd/contact-info-list"],
	"editorScript": "file:../../../../assets/js/blocks/contact-info-list-item.js",
	"editorStyle": "file:../../../../assets/css/blocks/contact-info-list-item.css",
	"style": "file:../../../../assets/css/blocks/style-contact-info-list-item.css",
	"attributes": {
		"content": {
			"type": "string",
			"default": ""
		},
		"type": {
			"type": "string",
			"default": "email"
		},
		"post": {
			"type": "object"
		},
		"linkUrl": {
			"type": "string",
			"default": ""
		},
		"linkTitle": {
			"type": "string",
			"default": ""
		},
		"linkTarget": {
			"type": "string"
		},
		"linkRel": {
			"type": "string"
		}
	}
}
