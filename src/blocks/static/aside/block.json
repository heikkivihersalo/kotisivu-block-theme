{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 3,
	"name": "ksd/aside",
	"title": "Aside",
	"category": "containers",
	"icon": "layout",
	"description": "",
	"keywords": ["container, columns, column, aside"],
	"textdomain": "kotisivu-block-theme",
	"supports": {
		"className": false,
		"anchor": true,
		"html": true,
		"color": {
			"text": false,
			"background": true
		},
		"spacing": {
			"margin": ["top", "bottom"],
			"padding": true
		},
		"ariaLabel": true,
		"align": ["full"]
	},
	"editorScript": "file:../../../../assets/js/blocks/aside.js",
	"editorStyle": "file:../../../../assets/css/blocks/aside.css",
	"style": "file:../../../../assets/css/blocks/style-aside.css",
	"attributes": {
		"blockClass": {
			"type": "string",
			"default": ""
		},
		"style": {
			"type": "object",
			"default": {}
		},
		"ariaLabel": {
			"type": "string"
		},
		"ariaLabelledBy": {
			"type": "string"
		},
		"template": {
			"type": "array",
			"default": []
		},
		"allowedBlocks": {
			"type": "array"
		},
		"templateLock": {
			"type": ["string", "boolean"],
			"enum": ["all", "insert", false]
		},
		"isReversed": {
			"type": "boolean",
			"default": false
		},
		"variationName": {
			"type": "string",
			"default": ""
		}
	}
}
