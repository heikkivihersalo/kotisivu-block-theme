{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "ksd/column",
	"title": "Column New",
	"category": "text",
	"parent": [ "ksd/columns" ],
	"description": "A single column within a columns block.",
	"textdomain": "kotisivu-block-theme",
	"editorScript": "file:../../../../build/blocks/column.js",
    "editorStyle": "file:../../../../build/blocks/column.css",
    "style": "file:../../../../build/blocks/style-column.css",
	"attributes": {
		"allowedBlocks": {
			"type": "array"
		},
		"templateLock": {
			"type": [ "string", "boolean" ],
			"enum": [ "all", "insert", "contentOnly", false ]
		}
	},
	"supports": {
		"anchor": true
	}
}
