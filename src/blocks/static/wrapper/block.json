{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 3,
	"name": "ksd/wrapper",
	"title": "Wrapper",
	"category": "containers",
	"icon": "layout",
	"description": "",
	"keywords": ["container, columns, column, wrapper"],
	"textdomain": "kotisivu-block-theme",
	"supports": {
		"className": false,
		"anchor": true,
		"html": true,
		"color": {
			"text": false,
			"background": true
		},
		"spacing": {
			"margin": ["top", "bottom"],
			"padding": true
		},
		"ariaLabel": true,
		"align": ["full"]
	},
	"editorScript": "file:../../../../build/blocks/wrapper.js",
	"editorStyle": "file:../../../../build/blocks/wrapper.css",
	"style": "file:../../../../build/blocks/style-wrapper.css",
	"attributes": {
		"wrapperClass": {
			"type": "string",
			"default": ""
		},
		"style": {
			"type": "object",
			"default": {}
		},
		"template": {
			"type": "array",
			"default": []
		},
		"allowedBlocks": {
			"type": "array"
		},
		"templateLock": {
			"type": ["string", "boolean"],
			"enum": ["all", "insert", false]
		},
		"width": {
			"enum": [
				"var(--wp--style--global--wide-size)",
				"var(--wp--style--global--content-size)"
			]
		},
		"alignItems": {
			"enum": ["center", "start", "end", "stretch", "baseline"]
		},
		"justifyItems": {
			"enum": ["center", "start", "end", "stretch"]
		},
		"isReversed": {
			"type": "boolean",
			"default": false
		},
		"showAlignmentControls": {
			"type": "boolean",
			"default": true
		},
		"variationName": {
			"type": "string",
			"default": ""
		}
	}
}
