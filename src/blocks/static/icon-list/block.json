{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ksd/icon-list",
	"title": "Icon List",
	"category": "widgets",
	"icon": "list-view",
	"description": "Icon list with insertable image",
	"keywords": [ "container, icon, list" ],
	"textdomain": "kotisivu-block-theme",
	"supports": {
		"className": false
	},
	"editorScript": "file:../../../../build/icon-list.js",
    "editorStyle": "file:../../../../build/icon-list.css",
    "style": "file:../../../../build/style-icon-list.css",
	"attributes": {
		"template": {
			"type": "array",
			"default": [
				["ksd/icon-list-item"],
				["ksd/icon-list-item"]
			]
		},
		"allowedBlocks": {
			"type": "array",
			"default": ["ksd/icon-list-item"]
		},
		"templateLock": {
			"type": [ "string", "boolean" ],
			"enum": [ "all", "insert", false ],
			"default": false
		}
	}
}
