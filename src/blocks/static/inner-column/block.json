{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ksd/inner-column",
	"title": "Inner Column",
	"category": "design",
	"icon": "columns",
	"description": "Optimized column for column container",
	"keywords": ["columns, column"],
	"textdomain": "kotisivu-block-theme",
	"supports": {
		"className": false
	},
	"parent": ["ksd/column-container"],
	"editorScript": "file:../../../../build/blocks/inner-column.js",
	"editorStyle": "file:../../../../build/blocks/inner-column.css",
	"style": "file:../../../../build/blocks/style-inner-column.css",
	"attributes": {
		"template": {
			"type": "array",
			"default": []
		},
		"allowedBlocks": {
			"type": "array"
		},
		"templateLock": {
			"type": [ "string", "boolean" ],
			"enum": [ "all", "insert", false ]
		}
	}
}
