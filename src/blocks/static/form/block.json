{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ksd/form",
	"title": "Form",
	"category": "widgets",
	"icon": "editor-table",
	"description": "Form",
	"keywords": ["form, contact form"],
	"textdomain": "kotisivu-block-theme",
	"supports": {
		"className": false,
		"anchor": true,
		"html": true,
		"color": {
			"text": false,
			"background": true
		},
		"spacing": {
			"margin": ["top", "bottom"],
			"padding": true
		}
	},
	"editorScript": "file:../../../../assets/js/blocks/form.js",
	"editorStyle": "file:../../../../assets/css/blocks/form.css",
	"style": "file:../../../../assets/css/blocks/style-form.css",
	"attributes": {
		"blockClass": {
			"type": "string",
			"default": "contact-us"
		},
		"style": {
			"type": "object",
			"default": {
				"margin": {
					"top": "var(--wp--preset--spacing--50)",
					"bottom": "var(--wp--preset--spacing--50)"
				}
			}
		},
		"ariaLabel": {
			"type": "string"
		},
		"ariaLabelledBy": {
			"type": "string"
		},
		"template": {
			"type": "array",
			"default": [
				["core/heading", { "placeholder": "Otsikko", "level": 2 }],
				["core/shortcode", { "text": "[fluentform id='3']" }]
			]
		},
		"templateLock": {
			"type": ["string", "boolean"],
			"enum": ["all", "insert", false]
		}
	}
}
