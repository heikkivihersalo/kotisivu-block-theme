{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ksd/container-100",
	"title": "Container 100",
	"category": "design",
	"icon": "layout",
	"description": "Container for one column sections",
	"keywords": [ "container, columns, column, 100" ],
	"textdomain": "kotisivu-block-theme",
	"supports": {
		"className": false
	},
	"editorScript": "file:../../../../build/blocks/container-100.js",
    "editorStyle": "file:../../../../build/blocks/container-100.css",
    "style": "file:../../../../build/blocks/style-container-100.css",
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"name": {
			"type": "string",
			"default": ""
		},
		"container": {
			"type": "string",
			"default": "div"
		},
		"modifiers": {
			"type": "string",
			"default": ""
		},
		"isReversed": {
			"type": "boolean"
		},
		"hasBackgroundColor": {
			"type": "boolean"
		},
		"backgroundColor": {
			"type": "string"
		},
		"template": {
			"type": "array",
			"default": []
		},
		"allowedBlocks": {
			"type": "array"
		},
		"templateLock": {
			"type": [ "string", "boolean" ],
			"enum": [ "all", "insert", false ]
		},
		"align": {
			"type": "string",
			"default": "full"
		}
	}
}
