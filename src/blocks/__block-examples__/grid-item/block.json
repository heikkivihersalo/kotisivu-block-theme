{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "ksd/grid-item",
	"title": "Grid Item",
	"category": "theme",
	"icon": "cover-image",
	"description": "",
	"keywords": [""],
	"textdomain": "kotisivu-block-theme",
	"supports": {
		"className": false,
		"html": false,
		"spacing": {
			"margin": ["top", "bottom"],
			"padding": true
		}
	},
	"parent": ["ksd/grid"],
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"style": {
			"type": "object"
		},
		"blockClass": {
			"type": "string"
		},
		"template": {
			"type": "array",
			"default": []
		},
		"allowedBlocks": {
			"type": "array"
		},
		"templateLock": {
			"type": ["string", "boolean"],
			"enum": ["all", "insert", false]
		}
	}
}
