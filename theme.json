{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": false,
    "useRootPaddingAwareAlignments": true,
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false
    },
    "color": {
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "duotone": [],
      "gradients": [],
      "link": false,
      "palette": [
        {
          "slug": "primary",
          "color": "hsl(343, 100%, 48%)",
          "name": "Primary"
        },
        {
          "slug": "primary-dark",
          "color": "hsl(343, 100%, 28%)",
          "name": "Primary (Dark)"
        },
        {
          "slug": "secondary",
          "color": "hsl(163, 100%, 48%)",
          "name": "Secondary"
        },
        {
          "slug": "background",
          "color": "hsl(0, 0%, 100%)",
          "name": "Background"
        },
        {
          "slug": "foreground",
          "color": "hsl(180, 5%, 8%)",
          "name": "Foreground"
        },
        {
          "slug": "white",
          "color": "hsl(0, 0%, 100%)",
          "name": "White"
        },
        {
          "slug": "black",
          "color": "hsl(180, 5%, 8%)",
          "name": "Black"
        },
        {
          "slug": "grey-light",
          "color": "hsl(0, 0%, 95%)",
          "name": "Light Grey"
        },
        {
          "slug": "grey",
          "color": "hsl(233, 4%, 52%)",
          "name": "Grey"
        },
        {
          "slug": "grey-dark",
          "color": "hsl(233, 4%, 22%)",
          "name": "Dark Grey"
        }
      ],
      "text": true,
      "background": false,
      "defaultGradients": false,
      "defaultPalette": false
    },
    "layout": {
      "contentSize": "min(calc(100% - calc(var(--wp--preset--spacing--40)) * 2), 1250px)",
      "wideSize": "min(100%, 1920px)"
    },
    "spacing": {
      "units": ["em", "rem", "%", "vw", "vh"],
      "blockGap": null,
      "margin": true,
      "padding": true,
      "spacingSizes": [
        {
          "size": "clamp(0.618046971569839rem, calc(0.615109684219015rem + 0.0146864367541203vw), 0.63273340832396rem)",
          "slug": "20",
          "name": "1"
        },
        {
          "size": "clamp(1rem, calc(0.975rem + 0.125vw), 1.125rem)",
          "slug": "30",
          "name": "2"
        },
        {
          "size": "clamp(1.618rem, calc(1.54155rem + 0.38225vw), 2.00025rem)",
          "slug": "40",
          "name": "3"
        },
        {
          "size": "clamp(2.617924rem, calc(2.4302199rem + 0.9385205vw), 3.5564445rem)",
          "slug": "50",
          "name": "4"
        },
        {
          "size": "clamp(4.235801032rem, calc(3.8182895742rem + 2.087557289vw), 6.323358321rem)",
          "slug": "60",
          "name": "5"
        },
        {
          "size": "clamp(6.853526069776rem, calc(5.9756450647836rem + 4.389405024962vw), 11.242931094738rem)",
          "slug": "70",
          "name": "6"
        },
        {
          "size": "clamp(11.0890051808976rem, calc(9.30881991978825rem + 8.90092630554659vw), 19.9899314864442rem)",
          "slug": "80",
          "name": "7"
        },
        {
          "size": "clamp(17.9420103826923rem, calc(14.4219928226512rem + 17.6000878002055vw), 35.5420981828977rem)",
          "slug": "90",
          "name": "8"
        }
      ]
    },
    "typography": {
      "fluid": true,
      "fontFamilies": [
        {
          "fontFamily": "\"Synonym\"",
          "name": "Synonym",
          "slug": "primary",
          "fontFace": [
            {
              "fontFamily": "Synonym",
              "fontWeight": "700",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./public/fonts/Synonym/Synonym-Bold.woff2"]
            },
            {
              "fontFamily": "Synonym",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./public/fonts/Synonym/Synonym-Regular.woff2"]
            },
            {
              "fontFamily": "Synonym",
              "fontWeight": "200",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./public/fonts/Synonym/Synonym-Extralight.woff2"]
            }
          ]
        },
        {
          "fontFamily": "\"Alpino\"",
          "name": "Alpino",
          "slug": "secondary",
          "fontFace": [
            {
              "fontFamily": "Alpino",
              "fontWeight": "900",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./public/fonts/Alpino/Alpino-Black.woff2"]
            },
            {
              "fontFamily": "Alpino",
              "fontWeight": "700",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./public/fonts/Alpino/Alpino-Bold.woff2"]
            }
          ]
        }
      ],
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "textDecoration": false,
      "lineHeight": false,
      "textTransform": false,
      "dropCap": false,
      "customFontSize": false,
      "fontSizes": [
        {
          "name": "Colossal",
          "size": "2.488rem",
          "slug": "colossal",
          "fluid": {
            "min": "2.488rem",
            "max": "4.735rem"
          }
        },
        {
          "name": "Gigantic",
          "size": "2.074rem",
          "slug": "gigantic",
          "fluid": {
            "min": "2.074rem",
            "max": "3.552rem"
          }
        },
        {
          "name": "Huge",
          "size": "1.728rem",
          "slug": "huge",
          "fluid": {
            "min": "1.728rem",
            "max": "2.665rem"
          }
        },
        {
          "name": "Extra Large",
          "size": "1.440rem",
          "slug": "x-large",
          "fluid": {
            "min": "1.440rem",
            "max": "1.999rem"
          }
        },
        {
          "name": "Large",
          "size": "1.200rem",
          "slug": "large",
          "fluid": {
            "min": "1.200rem",
            "max": "1.500rem"
          }
        },
        {
          "name": "Medium",
          "size": "1rem",
          "slug": "medium",
          "fluid": {
            "min": "1rem",
            "max": "1.125rem"
          }
        },
        {
          "name": "Small",
          "size": "0.844rem",
          "slug": "small",
          "fluid": {
            "min": "0.844rem",
            "max": "0.844rem"
          }
        },
        {
          "name": "Tiny",
          "size": "0.633rem",
          "slug": "tiny",
          "fluid": {
            "min": "0.633rem",
            "max": "0.633rem"
          }
        }
      ]
    },
    "blocks": {
      "core/paragraph": {
        "spacing": {
          "margin": true
        }
      },
      "core/heading": {
        "spacing": {
          "margin": true
        }
      },
      "core/button": {
        "border": {
          "radius": false
        }
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--foreground)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--primary)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "fontWeight": "400",
      "lineHeight": "1.7"
    },
    "elements": {
      "heading": {
				"typography": {
          "fontFamily": "var(--wp--preset--font-family--secondary)",
          "fontWeight": "700",
          "lineHeight": "1.2"
				},
        "spacing": {
          "margin": {
            "top": "0.5em",
            "bottom": "0.5em"
          }
        }
			},
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--colossal)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--gigantic)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--huge)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--foreground)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--primary-dark)"
          },
          "typography": {
            "textDecoration": "none"
          }
        },
        ":focus": {
          "typography": {
            "textDecoration": "underline dashed"
          }
        },
        ":active": {
          "color": {
            "text": "var(--wp--preset--color--primary-dark)"
          },
          "typography": {
            "textDecoration": "none"
          }
        },
        "typography": {
          "textDecoration": "none"
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    },
    {
      "name": "page-content",
      "title": "Page Content",
      "area": "uncategorized"
    },
    {
      "name": "single-content",
      "title": "Blog Content",
      "area": "uncategorized"
    },
    {
      "name": "form",
      "title": "Form",
      "area": "uncategorized"
    }
  ]
}
