{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": false,
    "useRootPaddingAwareAlignments": true,
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false
    },
    "color": {
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "duotone": [],
      "gradients": [],
      "link": false,
      "palette": [
        {
          "slug": "primary-light",
          "color": "hsl(188, 69%, 43%)",
          "name": "Light Primary"
        },
        {
          "slug": "primary",
          "color": "hsl(188, 69%, 28%)",
          "name": "Primary"
        },
        {
          "slug": "primary-dark",
          "color": "hsl(188, 69%, 13%)",
          "name": "Dark Primary"
        },
        {
          "slug": "secondary",
          "color": "hsl(29, 54%, 61%)",
          "name": "Secondary"
        },
        {
          "slug": "background-lightest",
          "color": "hsl(213, 40%, 35%)",
          "name": "Lightest Background"
        },
        {
          "slug": "background-light",
          "color": "hsl(213, 50%, 20%)",
          "name": "Light Background"
        },
        {
          "slug": "background",
          "color": "hsl(219, 47%, 15%)",
          "name": "Background"
        },
        {
          "slug": "background-dark",
          "color": "hsl(219, 47%, 12%)",
          "name": "Dark Background"
        },
        {
          "slug": "background-darkest",
          "color": "hsl(219, 47%, 9%)",
          "name": "Darkest Background"
        },
        {
          "slug": "foreground",
          "color": "hsl(0, 0%, 100%)",
          "name": "Foreground"
        },
        {
          "slug": "white",
          "color": "hsl(0, 0%, 100%)",
          "name": "White"
        },
        {
          "slug": "black",
          "color": "hsl(0, 0%, 0%)",
          "name": "Black"
        }
      ],
      "text": true,
      "background": false,
      "defaultGradients": false,
      "defaultPalette": false
    },
    "custom": {
      "site": {
        "width": "1400px",
        "fullWidth": "1920px",
        "maxWidth": "calc(100% - calc(var(--wp--preset--spacing--40)) * 2"
      }
    },
    "layout": {
      "contentSize": "min(var(--wp--custom--site--max-width), var(--wp--custom--site--width))",
      "wideSize": "min(100%, var(--wp--custom--site--full-width))"
    },
    "spacing": {
      "units": ["em", "rem", "%", "vw", "vh"],
      "blockGap": null,
      "margin": false,
      "padding": false,
      "spacingSizes": [
        {
          "size": "clamp(0.618046971569839rem, calc(0.615109684219015rem + 0.0146864367541203vw), 0.63273340832396rem)",
          "slug": "20",
          "name": "1"
        },
        {
          "size": "clamp(1rem, calc(0.975rem + 0.125vw), 1.125rem)",
          "slug": "30",
          "name": "2"
        },
        {
          "size": "clamp(1.618rem, calc(1.54155rem + 0.38225vw), 2.00025rem)",
          "slug": "40",
          "name": "3"
        },
        {
          "size": "clamp(2.617924rem, calc(2.4302199rem + 0.9385205vw), 3.5564445rem)",
          "slug": "50",
          "name": "4"
        },
        {
          "size": "clamp(4.235801032rem, calc(3.8182895742rem + 2.087557289vw), 6.323358321rem)",
          "slug": "60",
          "name": "5"
        },
        {
          "size": "clamp(6.853526069776rem, calc(5.9756450647836rem + 4.389405024962vw), 11.242931094738rem)",
          "slug": "70",
          "name": "6"
        },
        {
          "size": "clamp(11.0890051808976rem, calc(9.30881991978825rem + 8.90092630554659vw), 19.9899314864442rem)",
          "slug": "80",
          "name": "7"
        },
        {
          "size": "clamp(17.9420103826923rem, calc(14.4219928226512rem + 17.6000878002055vw), 35.5420981828977rem)",
          "slug": "90",
          "name": "8"
        }
      ]
    },
    "typography": {
      "fluid": true,
      "fontFamilies": [
        {
          "fontFamily": "\"Unna\"",
          "name": "Unna",
          "slug": "primary",
          "fontFace": [
            {
              "fontFamily": "Unna",
              "fontWeight": "700",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Unna-700.woff2"]
            },
            {
              "fontFamily": "Unna",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Unna-400.woff2"]
            }
          ]
        },
        {
          "fontFamily": "\"Sen\"",
          "name": "Sen",
          "slug": "secondary",
          "fontFace": [
            {
              "fontFamily": "Sen",
              "fontWeight": "700",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Sen-700.woff2"]
            },
            {
              "fontFamily": "Sen",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/Sen-400.woff2"]
            }
          ]
        }
      ],
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "textDecoration": false,
      "lineHeight": false,
      "textTransform": false,
      "dropCap": false,
      "customFontSize": false,
      "fontSizes": [
        {
          "name": "Colossal",
          "size": "2.488rem",
          "slug": "colossal",
          "fluid": {
            "min": "5.653rem",
            "max": "9.375rem"
          }
        },
        {
          "name": "Gigantic",
          "size": "2.074rem",
          "slug": "gigantic",
          "fluid": {
            "min": "2.074rem",
            "max": "4.625rem"
          }
        },
        {
          "name": "Huge",
          "size": "1.728rem",
          "slug": "huge",
          "fluid": {
            "min": "1.728rem",
            "max": "2.625rem"
          }
        },
        {
          "name": "Extra Large",
          "size": "1.440rem",
          "slug": "x-large",
          "fluid": {
            "min": "1.440rem",
            "max": "1.875rem"
          }
        },
        {
          "name": "Large",
          "size": "1.200rem",
          "slug": "large",
          "fluid": {
            "min": "1.200rem",
            "max": "1.250rem"
          }
        },
        {
          "name": "Medium",
          "size": "1rem",
          "slug": "medium",
          "fluid": {
            "min": "1rem",
            "max": "1.063rem"
          }
        },
        {
          "name": "Small",
          "size": "0.833rem",
          "slug": "small",
          "fluid": {
            "min": "0.833rem",
            "max": "0.938rem"
          }
        },
        {
          "name": "Tiny",
          "size": "0.694rem",
          "slug": "tiny",
          "fluid": {
            "min": "0.694rem",
            "max": "0.781rem"
          }
        }
      ]
    },
    "blocks": {
      "core/paragraph": {
        "spacing": {
          "margin": true
        }
      },
      "core/heading": {
        "spacing": {
          "margin": true
        }
      },
      "core/button": {
        "spacing": {
          "margin": true
        },
        "border": {
          "color": false,
          "radius": false,
          "style": false,
          "width": false
        },
        "color":{
          "text": false,
          "background": false,
          "customDuotone": false,
          "customGradient": false,
          "defaultDuotone": false,
          "defaultGradients": false,
          "duotone": [],
          "gradients": [],
          "palette": []
        },
        "typography": {
          "fontSizes": [],
          "fontFamilies": []
        }
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--foreground)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--primary)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "fontWeight": "400",
      "lineHeight": "1.7"
    },
    "elements": {
      "heading": {
				"typography": {
          "fontFamily": "var(--wp--preset--font-family--secondary)",
          "fontWeight": "700",
          "lineHeight": "1.2"
				},
        "spacing": {
          "margin": {
            "top": "0.5em",
            "bottom": "0.5em"
          }
        }
			},
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--colossal)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--gigantic)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--huge)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--foreground)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--primary-dark)"
          },
          "typography": {
            "textDecoration": "none"
          }
        },
        ":focus": {
          "typography": {
            "textDecoration": "underline dashed"
          }
        },
        ":active": {
          "color": {
            "text": "var(--wp--preset--color--primary-dark)"
          },
          "typography": {
            "textDecoration": "none"
          }
        },
        "typography": {
          "textDecoration": "none"
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    },
    {
      "name": "page-content",
      "title": "Page Content",
      "area": "uncategorized"
    },
    {
      "name": "single-content",
      "title": "Blog Content",
      "area": "uncategorized"
    },
    {
      "name": "form",
      "title": "Form",
      "area": "uncategorized"
    }
  ]
}
