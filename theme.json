				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary-dark)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--primary-dark)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				"typography": {
